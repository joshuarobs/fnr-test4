  "scripts": {
    "nx serve app": "npx nx serve fnr-app --skip-nx-cache",
    "nx serve server": "npx nx serve fnr-server --skip-nx-cache",
    "prisma:studio": "cd apps/fnr-server && npx prisma studio",
    "test:api": "npx nx test fnr-server-e2e --skip-nx-cache",
    "nx storybook": "npx nx storybook fnr-app",
    "nx graph app": "npx nx show project fnr-app",
    "nx reset": "npx nx reset",
    "nx build server": "npx nx build fnr-server --skip-nx-cache",
    "db:seed": "cd apps/fnr-server && npx tsx prisma/seed.ts",
    "db:reset": "cd apps/fnr-server && npx prisma db push --force-reset",
    "db:reset-and-seed": "npm run db:reset && npm run db:seed",
    "deploy": "bash scripts/quick-deploy.sh",
    "deploy:prod": "bash scripts/quick-deploy.sh %npm_config_ip% %npm_config_key%",
    "setup:server": "bash scripts/server-setup.sh",
    "setup:server:prod": "bash scripts/server-setup.sh %npm_config_ip% %npm_config_key%",
    "verify:deployment": "bash scripts/verify-deployment.sh",
    "verify:deployment:prod": "bash scripts/verify-deployment.sh %npm_config_ip% %npm_config_key%"
  },
  "prisma": {
    "seed": "cd apps/fnr-server && ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
